{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "allowUnreachableCode": false,
    "useUnknownInCatchVariables": false,
    "baseUrl": ".",
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "webpack.config.js"
  ],
}
